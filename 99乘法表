//九九乘法表一
#include <stdio.h>
int main(){
    int x,y,z=0;
    for (x=1;x<=9;x++){
        for(y=x;y<=9;y++){
            z=x*y;
            if(y==x){
                printf("\n");
            }
            printf("%d  ",z);
        }
    }
    return 0;
}

//九九乘法表二
#include <stdio.h>
int main(){
    int x,y,z=0;
    for(x=1;x<=9;x++){
        for(y=1;y<=9;y++){
            if (y>x){
                printf("\n");
                break;
            }
            z=x*y;
            printf("%d ",z);
        }
    }
    return 0;
}
